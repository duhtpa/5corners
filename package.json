{
  "name": "5corners",
  "version": "1.0.0",
  "description": "Тестовое задание для компании 5 углов",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "author": "dor_alex",
  "license": "ISC",
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
